  issn = {1520-8540},
  doi = {10.1364/JOSAB.36.000A93},
  abstract = {We report on a few-cycle, carrier\&\#x2013;envelope-phase-stable laser source based on supercontinuum generation driven by an amplified Er:fiber-based system. Laser pulses from an Er:fiber oscillator are amplified, and these amplified pulses generate a stable supercontinuum in a highly nonlinear optical fiber. The short- and long-wavelength tails of this continuum are used in an f-to-2f interferometer to stabilize the carrier\&\#x2013;envelope phase (CEP) of the Er:fiber oscillator via an acousto-optic modulator. Compressing the central part of the continuum, we generate a train of CEP-stabilized laser pulses with a central wavelength of 1170\&\#x00A0;nm, duration of 9.1\&\#x00A0;fs (\&\#x223C;2.3 optical cycles), and repetition rate of 78.4\&\#x00A0;MHz. Characterizing the CEP stability of our output pulse train with an out-of-loop f-to-2f interferometer, we find a phase jitter of only 157.4\&\#x00A0;mrad when integrating the radiofrequency spectrum from 5\&\#x00A0;mHz to 5\&\#x00A0;MHz.},
  copyright = {\&\#169; 2019 Optical Society of America},
  langid = {english},
  keywords = {Fiber lasers,Laser sources,Phase noise,Phase shift,Solid state lasers,Strong field physics},
  file = {/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/putnamFewcycleCarrierenvelopephasestableLaser2019.pdf;/home/dkeathley/Zotero/storage/UAXTLQY3/abstract.html}
}

