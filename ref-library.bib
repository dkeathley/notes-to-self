  langid = {english},
  keywords = {even,harmonics,hhg,metasurfaces,nanophotonics,nanostructures,odd,resonant,tunneling},
  annotation = {Bandiera\_abtest: a Cc\_license\_type: cc\_by Cg\_type: Nature Research Journals Primary\_atype: Research Subject\_term: High-harmonic generation;Metamaterials;Nonlinear optics;Photonic devices Subject\_term\_id: high-harmonic-generation;metamaterials;nonlinear-optics;photonic-devices},
