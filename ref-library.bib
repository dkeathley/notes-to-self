@article{keathleyVanishingCarrierenvelopephasesensitiveResponse2019a,
  title = {Vanishing Carrier-Envelope-Phase-Sensitive Response in Optical-Field Photoemission from Plasmonic Nanoantennas},
  author = {Keathley, P. D. and Putnam, W. P. and Vasireddy, P. and Hobbs, R. G. and Yang, Y. and Berggren, K. K. and K{\"a}rtner, F. X.},
