@article{ngEfficientRoomtemperatureSiliconbased2001,
  title = {An Efficient Room-Temperature Silicon-Based Light-Emitting Diode},
  author = {Ng, Wai Lek and Louren{\c c}o, M. A. and Gwilliam, R. M. and Ledain, S. and Shao, G. and Homewood, K. P.},
  year = {2001},
