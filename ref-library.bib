@article{goulielmakisSingleCycleNonlinearOptics2008,
  title = {Single-{{Cycle Nonlinear Optics}}},
  author = {Goulielmakis, E. and Schultze, M. and Hofstetter, M. and Yakovlev, V. S. and Gagnon, J. and Uiberacker, M. and Aquila, A. L. and Gullikson, E. M. and Attwood, D. T. and Kienberger, R. and Krausz, F. and Kleineberg, U.},
  year = {2008},
  month = jun,
  journal = {Science},
  volume = {320},
  number = {5883},
  pages = {1614--1617},
  doi = {10.1126/science.1157846},
  abstract = {Nonlinear optics plays a central role in the advancement of optical science and laser-based technologies. We report on the confinement of the nonlinear interaction of light with matter to a single wave cycle and demonstrate its utility for time-resolved and strong-field science. The electric field of 3.3-femtosecond, 0.72-micron laser pulses with a controlled and measured waveform ionizes atoms near the crests of the central wave cycle, with ionization being virtually switched off outside this interval. Isolated sub-100-attosecond pulses of extreme ultraviolet light (photon energy [\textasciitilde ] 80 electron volts), containing [\textasciitilde ]0.5 nanojoule of energy, emerge from the interaction with a conversion efficiency of [\textasciitilde ]10-6. These tools enable the study of the precision control of electron motion with light fields and electron-electron interactions with a resolution approaching the atomic unit of time ([\textasciitilde ]24 attoseconds).},
  lccn = {0199},
  keywords = {\#duplicates},
  file = {/home/dkeathley/Zotero/storage/54IK8BCT/single-cycle_nonlinear_optics_goulielmakis_schultze_science.pdf;/home/dkeathley/Zotero/storage/UF5J55A5/Goulielmakis et al. - 2008 - Single-Cycle Nonlinear Optics.pdf;/home/dkeathley/Zotero/storage/T6A6ZDF4/1614.html;/home/dkeathley/Zotero/storage/WRXA4P2I/1614.html}
