  langid = {english},
  keywords = {coherent,euv,imaging},
  file = {/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/bakshQuantitativeCorrelativeExtreme2020.pdf;/home/dkeathley/Zotero/storage/X79INXEG/tab-pdf.html}
