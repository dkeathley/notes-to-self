  keywords = {Electromagnetic radiation,Hyperspectral imaging,Imaging systems,Imaging techniques,Optical systems,Terahertz imaging},
  file = {/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/sterczewskiTerahertzHyperspectralImaging2019.pdf;/home/dkeathley/Zotero/storage/8XUY9MEQ/abstract.html}
