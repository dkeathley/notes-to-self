@article{zheltikovLaserinducedTunnelingKapitza2019,
  title = {Laser-Induced Tunneling, the {{Kapitza}} Effective Potential, and the Limits of Perturbative Nonlinear Optics},
  author = {Zheltikov, Aleksei},
  year = {2019},
  month = mar,
  journal = {Optics Express},
  volume = {27},
  number = {6},
  pages = {8246--8257},
  issn = {1094-4087},
  doi = {10.1364/OE.27.008246},
  abstract = {The canonical framework of optical physics connects the validity of perturbative nonlinear optics to the smallness of the optical driver field E compared to a characteristic field Eat that acts on electrons in an atom, a molecule, or a crystal lattice. However, in a vast area of strong-field optical science, the borderline between perturbative and nonperturbative nonlinear optics is defined as \&\#x03B3; \textasciitilde 1, where \&\#x03B3; is the Keldysh parameter. Not only is this criterion frequency-dependent, in a stark contrast with E/Eat \textasciitilde 1, but it also often dictates much weaker fields at which the perturbative treatment is still valid, leading to a dramatic shrinkage in the convergence radius of perturbation-theory expansions. Here, we identify the physics behind the gap between the E/Eat \textasciitilde 1 and \&\#x03B3; \textasciitilde 1 conditions as the limits of perturbative nonlinear optics. We argue that, while the criterion E/Eat \&lt;\&lt; 1 sets a universal upper-bound limit on the validity of perturbative nonlinear optics and its central concept of nonlinear-optical susceptibilities, optical nonlinearities related to photoionization pathways become nonperturbative in much weaker optical fields, with the limits of a perturbative treatment defined by the Keldysh parameter \&\#x03B3; rather than the E/Eat ratio.},
  copyright = {\&\#169; 2019 Optical Society of America},
  langid = {english},
  keywords = {Absorption coefficient,Electric fields,Laser filaments,Multiphoton ionization,Nonlinear optics,Photoionization},
  file = {/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/zheltikovLaserinducedTunnelingKapitza2019.pdf;/home/dkeathley/Zotero/storage/CKNWQ5JT/abstract.html}
}

