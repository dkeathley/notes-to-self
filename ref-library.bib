  author = {Carbajo, Sergio and Nanni, Emilio A. and Wong, Liang Jie and Moriena, Gustavo and Keathley, Phillip D. and Laurent, Guillaume and Miller, R. J. Dwayne and K{\"a}rtner, Franz X.},
  year = {2016},
