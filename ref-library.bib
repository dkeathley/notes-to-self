  langid = {english},
  file = {/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/tancogne-dejeanAtomiclikeHighharmonicGeneration2018.pdf;/home/dkeathley/Zotero/storage/8UQWX4IC/eaao5207.html}
}

