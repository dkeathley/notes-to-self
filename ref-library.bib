  keywords = {Diffraction efficiency,Diffraction gratings,Nonlinear susceptibility,Picosecond pulses,Rough surface scattering,Surface plasmon polaritons},
  file = {/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/inchaussandagueSecondharmonicGenerationPlasmon2017.pdf;/home/dkeathley/Zotero/storage/RLJBKPYZ/abstract.html}
