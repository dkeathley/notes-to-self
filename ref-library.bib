@article{landsmanUltrafastResolutionTunneling2014,
  title = {Ultrafast Resolution of Tunneling Delay Time},
  author = {Landsman, Alexandra S. and Weger, Matthias and Maurer, Jochen and Boge, Robert and Ludwig, Andr{\'e} and Heuser, Sebastian and Cirelli, Claudio and Gallmann, Lukas and Keller, Ursula},
  year = {2014},
