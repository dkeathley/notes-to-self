@article{yiExcitonDissociationChargeRecombinationProcesses2009,
  title = {Exciton-{{Dissociation}} and {{Charge-Recombination Processes}} in {{Pentacene}}/{{C60 Solar Cells}}: {{Theoretical Insight}} into the {{Impact}} of {{Interface Geometry}}},
  shorttitle = {Exciton-{{Dissociation}} and {{Charge-Recombination Processes}} in {{Pentacene}}/{{C60 Solar Cells}}},
