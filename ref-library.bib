@article{okawaPhotonHeterodyning2017,
  title = {Photon Heterodyning},
  author = {Okawa, Youhei and Omura, Fuminori and Yasutake, Yuhsuke and Fukatsu, Susumu},
  year = {2017},
  month = aug,
  journal = {Optics Express},
