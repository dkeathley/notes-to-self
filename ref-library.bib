@article{nardiNanoscaleRefractoryDoped2021,
  title = {Nanoscale Refractory Doped Titanium Nitride Field Emitters},
  author = {Nardi, A. and Turchetti, M. and Britton, W. A. and Chen, Y. and Yang, Y. and Negro, L. Dal and Berggren, K. K. and Keathley, P. D.},
