@article{feistCavitymediatedElectronphotonPairs2022,
  title = {Cavity-Mediated Electron-Photon Pairs},
  author = {Feist, A. and Huang, G. and Arend, G. and Yang, Y. and Henke, J.-W. and Raja, A. S. and Kappert, F. J. and Wang, R. N. and {Louren{\c c}o-Martins}, H. and Qiu, Z. and Liu, J. and Kfir, O. and Kippenberg, T. J. and Ropers, C.},
