@article{deninger75THzTuning2015,
  title = {2.75 {{THz}} Tuning with a Triple-{{DFB}} Laser System at 1550 Nm and {{InGaAs}} Photomixers},
  author = {Deninger, Anselm J. and Roggenbuck, A. and Schindler, S. and Preu, S.},
