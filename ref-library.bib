@article{limPotentialEnvironmentalImpacts2011,
  title = {Potential {{Environmental Impacts}} of {{Light-Emitting Diodes}} ({{LEDs}}): {{Metallic Resources}}, {{Toxicity}}, and {{Hazardous Waste Classification}}},
  shorttitle = {Potential {{Environmental Impacts}} of {{Light-Emitting Diodes}} ({{LEDs}})},
