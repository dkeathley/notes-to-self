@article{wagnerMultifrequencyDifferentialAbsorption2018,
  title = {Multi-Frequency Differential Absorption {{LIDAR}} System for Remote Sensing of {{CO}}{\textsubscript{2}} and {{H}}{\textsubscript{2}}{{O}} near 1.6 \&\#{{x00B5}};m},
  author = {Wagner, Gerd A. and Plusquellic, David F.},
