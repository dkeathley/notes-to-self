@article{riffeFemtosecondThermionicEmission1993,
  title = {Femtosecond Thermionic Emission from Metals in the Space-Charge-Limited Regime},
  author = {Riffe, D. M. and Wang, X. Y. and Downer, M. C. and Fisher, D. L. and Tajima, T. and Erskine, J. L. and More, R. M.},
