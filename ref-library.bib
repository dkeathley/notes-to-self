  langid = {english},
  pmid = {16946063},
  file = {/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/prokhorenkoCoherentControlRetinal2006.pdf;/home/dkeathley/Zotero/storage/6XX8YG9X/1257.html}
}

@article{psaliaresetalAngularDistributionsHighorder1996,
