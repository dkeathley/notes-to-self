  author = {Schiffrin, Agustin and {Paasch-Colberg}, Tim and Karpowicz, Nicholas and Apalkov, Vadym and Gerster, Daniel and M{\"u}hlbrandt, Sascha and Korbman, Michael and Reichert, Joachim and Schultze, Martin and Holzner, Simon and Barth, Johannes V. and Kienberger, Reinhard and Ernstorfer, Ralph and Yakovlev, Vladislav S. and Stockman, Mark I. and Krausz, Ferenc},
