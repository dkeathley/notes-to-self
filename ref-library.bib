@article{kimManipulationQuantumPaths2013,
  title = {Manipulation of Quantum Paths for Space-Time Characterization of Attosecond Pulses},
  author = {Kim, Kyung Taec and Zhang, Chunmei and Shiner, Andrew D. and Kirkwood, Sean E. and Frumker, Eugene and Gariepy, Genevieve and Naumov, Andrei and Villeneuve, D. M. and Corkum, P. B.},
  year = {print March 2013},
  journal = {Nature Physics},
  volume = {9},
  number = {3},
  pages = {159--163},
  issn = {1745-2473},
  doi = {10.1038/nphys2525},
  abstract = {Attosecond extreme-ultraviolet pulses have a complex space\textendash time structure. However, at present, there is no method to observe this intricate detail; all measurements of the duration of attosecond pulses are, to some extent, spatially averaged. A technique for determining the full space\textendash time structure would enable a detailed study of the highly nonlinear processes that generate these pulses as a function of intensity without averaging. Here, we introduce and demonstrate an all-optical method to measure the space\textendash time characteristics of an isolated attosecond pulse. Our measurements show that intensity-dependent phase and quantum-path interference both play a key role in determining the pulse structure. In the generating medium, the attosecond pulse is strongly modulated in space and time. Propagation modifies but does not erase this modulation. Quantum-path interference of the single-atom response, previously obscured by spatial and temporal averaging, may enable measuring the laser-field-driven ion dynamics with sub-cycle resolution.},
  copyright = {\textcopyright{} 2013 Nature Publishing Group},
  langid = {english},
  keywords = {Optical physics},
  file = {/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/kimManipulationQuantumPaths2013.pdf;/home/dkeathley/Zotero/storage/QHNDPH2D/nphys2525.html}
}

