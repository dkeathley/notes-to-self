  langid = {english},
  pmid = {29146807},
  file = {/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/juTunableExcitonsBilayer2017.pdf;/home/dkeathley/Zotero/storage/B3FVJ39U/tab-pdf.html}
