  langid = {english},
  file = {/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/buckleyNanoantennaDesignEnhanced2021.pdf;/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/buckleyNanoantennaDesignEnhanced2021a.pdf;/home/dkeathley/Zotero/storage/5LQ52MVJ/fulltext.html}
