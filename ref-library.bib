@article{hatoriGlobalRisePotential2017,
  title = {Global Rise of Potential Health Hazards Caused by Blue Light-Induced Circadian Disruption in Modern Aging Societies},
  author = {Hatori, Megumi and Gronfier, Claude and Gelder, Russell N. Van and Bernstein, Paul S. and Carreras, Josep and Panda, Satchidananda and Marks, Frederick and Sliney, David and Hunt, Charles E. and Hirota, Tsuyoshi and Furukawa, Toshiharu and Tsubota, Kazuo},
