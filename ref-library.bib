@article{putnamOpticalfieldcontrolledPhotoemissionPlasmonic2017,
  title = {Optical-Field-Controlled Photoemission from Plasmonic Nanoparticles},
  author = {Putnam, William P. and Hobbs, Richard G. and Keathley, Phillip D. and Berggren, Karl K. and K{\"a}rtner, Franz X.},
  year = {2017},
