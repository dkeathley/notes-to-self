@article{esmannPlasmonicNanofocusingSpectral2020,
  title = {Plasmonic Nanofocusing Spectral Interferometry},
  author = {Esmann, Martin and Chimeh, Abbas and Korte, Anke and Zhong, Jin-Hui and Stephan, Sven and Witt, Julia and Wittstock, Gunther and Talebi, Nahid and Lienau, Christoph},
