@article{weissComputedTomographyCryogenic2000,
  title = {Computed Tomography of Cryogenic Biological Specimens Based on {{X-ray}} Microscopic Images},
  author = {Wei{\ss}, D and Schneider, G and Niemann, B and Guttmann, P and Rudolph, D and Schmahl, G},
  year = {2000},
  month = aug,
  journal = {Ultramicroscopy},
  volume = {84},
