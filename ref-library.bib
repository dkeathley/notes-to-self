  title = {Midinfrared Supercontinuum Generation from 2 to 6\&\#x2009;\&\#x2009;\&\#{{x3BC}};m in a Silicon Nanowire},
  author = {Singh, Neetesh and Hudson, Darren D. and Yu, Yi and Grillet, Christian and Jackson, Stuart D. and {Casas-Bedoya}, Alvaro and Read, Andrew and Atanackovic, Petar and Duvall, Steven G. and Palomba, Stefano and {Luther-Davies}, Barry and Madden, Stephen and Moss, David J. and Eggleton, Benjamin J.},
