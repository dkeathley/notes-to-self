@article{prasadCorrelatingPhotonsUsing2020,
  title = {Correlating Photons Using the Collective Nonlinear Response of Atoms Weakly Coupled to an Optical Mode},
  author = {Prasad, Adarsh S. and Hinney, Jakob and Mahmoodian, Sahand and Hammerer, Klemens and Rind, Samuel and Schneeweiss, Philipp and S{\o}rensen, Anders S. and Volz, J{\"u}rgen and Rauschenbeutel, Arno},
