  author = {Shcherbakov, Maxim R. and Zhang, Haizhong and Tripepi, Michael and Sartorello, Giovanni and Talisa, Noah and AlShafey, Abdallah and Fan, Zhiyuan and Twardowski, Justin and Krivitsky, Leonid A. and Kuznetsov, Arseniy I. and Chowdhury, Enam and Shvets, Gennady},
  year = {2021},
