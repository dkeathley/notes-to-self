@article{liuEnhancedHighharmonicGeneration2018,
  title = {Enhanced High-Harmonic Generation from an All-Dielectric Metasurface},
  author = {Liu, Hanzhe and Guo, Cheng and Vampa, Giulio and Zhang, Jingyuan Linda and Sarmiento, Tomas and Xiao, Meng and Bucksbaum, Philip H. and Vu{\v c}kovi{\'c}, Jelena and Fan, Shanhui and Reis, David A.},
