  keywords = {information,memory,Nonlinear Sciences - Pattern Formation and Solitons,Physics - Optics,soliton},
  note = {Comment: 3 figures
\par
\section{Notes}

\par
Storing information in solitons similar to what has been proposed for molecular vibrations.},
  file = {/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/kurtzNonlinearSpectroscopyAllOptical2020.pdf;/home/dkeathley/Zotero/storage/WZ8ASCMI/1908.html}
}

