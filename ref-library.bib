@article{sungThreeDimensionalHolographicRefractiveIndex2014,
  title = {Three-{{Dimensional Holographic Refractive-Index Measurement}} of {{Continuously Flowing Cells}} in a {{Microfluidic Channel}}},
  author = {Sung, Yongjin and Lue, Niyom and Hamza, Bashar and Martel, Joseph and Irimia, Daniel and Dasari, Ramachandra R. and Choi, Wonshik and Yaqoob, Zahid and So, Peter},
  year = {2014},
  month = feb,
  journal = {Physical review applied},
  volume = {1},
  issn = {2331-7019},
  doi = {10.1103/PhysRevApplied.1.014002},
  abstract = {Refractive index of biological specimens is a source of intrinsic contrast that can be explored without any concerns of photobleaching or harmful effects caused by extra contrast agents. In addition, RI contains rich information related to the metabolism of cells at the cellular and subcellular levels. Here, we report a no-moving parts approach that provides three-dimensional refractive index maps of biological samples continuously flowing in a microfluidic channel. Specifically, we use line illumination and off-axis digital holography to record the angular spectra of light scattered from flowing samples at high speed. Applying the scalar diffraction theory, we obtain accurate RI maps of the samples from the measured spectra. Using this method, we demonstrate label-free 3-D imaging of live RKO human colon cancer cells and RPMI8226 multiple myeloma cells, and obtain the volume, dry mass and density of these cells from the measured 3-D refractive index maps. Our results show that the reported method, alone or in combination with the existing flow cytometry techniques, promises as a quantitative tool for stain-free characterization of large number of cells.},
  pmcid = {PMC4236915},
  pmid = {25419536},
  keywords = {biomolecules,cell,holography,index contrast,microscopy},
  note = {\section{Notes}

\begin{itemize}

\item Very similar to what I think we could do with the nonlinear antenna arrays if we went into imaging mode.

\item Advantages of the tip sensor:

\begin{itemize}

\item Back-side illumination is much easier configuration for measurments

\item The detection could include nonlinar information, not simply the linear refractive index response

\item It would be highly hyperspectral -- that would give much more molecular information without the need for tagging with fluorescent chemicals, even compared to what they do in this work

\end{itemize}

\end{itemize}},
  file = {/home/dkeathley/Dropbox (MIT)/Apps/GoodNotes 5/GoodNotes Backup/Library/sungThreeDimensionalHolographicRefractiveIndex2014.pdf}
}

